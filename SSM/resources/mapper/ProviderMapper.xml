<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.ProviderDao">
    <select id="queryAll" resultType="com.pojo.Provider">
        select * from smbms_provider
    </select>
    <select id="queryProCode" resultType="java.lang.String">
        select proCode from smbms_provider where proCode = #{proCode}
    </select>
    <insert id="add">
        insert into smbms_provider
        (proCode,proName,proDesc,proContact,proPhone,proAddress,
        proFax,createdBy,creationDate,modifyDate,modifyBy)
        values(#{proCode},#{proName},#{proDesc},#{proContact},#{proPhone},#{proAddress},
        #{proFax},#{createdBy},#{creationDate},#{modifyDate},#{modifyBy})
    </insert>
</mapper>